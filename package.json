{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Some basic ReactJS",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --port 3001 --history-api-fallback  --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Anirudh Khandelwal",
  "license": "MIT",
  "dependencies": {
    "connect": "^3.6.6",
    "prettier": "^1.11.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
